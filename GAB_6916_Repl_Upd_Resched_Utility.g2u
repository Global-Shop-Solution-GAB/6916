Program.Sub.cmdUpdPri_Click.Start
	F.Intrinsic.Control.Try
	V.Local.sError.Declare

	V.Local.sSelect.Declare
	V.Local.sSelect2.Declare
	V.Local.iCnt.Declare
	V.Local.iCnt2.Declare
	V.Local.sWOs.Declare
	V.Local.sSQL.Declare
	V.Local.sExp.Declare
	
	F.Data.DataTable.Select("dtWC","UpdPri = True",V.Local.sSelect)
	F.Intrinsic.Control.If(V.Local.sSelect.IsNotNoReturn)
		F.Intrinsic.String.Split(V.Local.sSelect,"*!*",V.Local.sSelect)
		F.Intrinsic.Control.For(V.Local.iCnt,0,V.Local.sSelect.UBound,1)
			F.Intrinsic.Control.If(V.Local.iCnt,=,0)
				F.Intrinsic.String.Build("'{0}'",V.DataTable.dtWC(V.Local.sSelect(V.Local.iCnt)).JOBSUF!FieldVal,V.Local.sWOs)
			F.Intrinsic.Control.Else
				F.Intrinsic.String.Build("{0},'{1}'",V.Local.sWOs,V.DataTable.dtWC(V.Local.sSelect(V.Local.iCnt)).JOBSUF!FieldVal,V.Local.sWOs)
			F.Intrinsic.Control.EndIf
			'Find and Update all matching WO Numbers
			F.Intrinsic.String.Build("JOBSUF = '{0}'",V.DataTable.dtWC(V.Local.sSelect(V.Local.iCnt)).JOBSUF!FieldVal,V.Local.sExp)
			F.Data.DataTable.Select("dtWC",V.Local.sExp,V.Local.sSelect2)
			F.Intrinsic.Control.If(V.Local.sSelect2.IsNotNoReturn)
				F.Intrinsic.String.Split(V.Local.sSelect2,"*!*",V.Local.sSelect2)
				F.Intrinsic.Control.For(V.Local.iCnt2,0,V.Local.sSelect2.UBound,1)
					F.Data.DataTable.SetValue("dtWC",V.Local.sSelect2(V.Local.iCnt2),"Priority",V.Screen.frmResched!txtUpdPri.Text,"Priority2",V.Screen.frmResched!txtUpdPri.Text)
				F.Intrinsic.Control.Next(V.Local.iCnt2)
			F.Intrinsic.Control.EndIf
		F.Intrinsic.Control.Next(V.Local.iCnt)
		F.Intrinsic.String.Build("Update JOB_HEADER Set PRIORITY = '{0}' Where JOB+SUFFIX IN ({1});",V.Screen.frmResched!txtUpdPri.Text,V.Local.sWOs,V.Local.sSQL)
		F.ODBC.Connection!con.Execute(V.Local.sSQL)
		F.Intrinsic.String.Build("Update APSV3_JBMASTER Set PRIORITY = '{0}' Where JOB+SFX IN ({1});",V.Screen.frmResched!txtUpdPri.Text,V.Local.sWOs,V.Local.sSQL)
		F.ODBC.Connection!con.Execute(V.Local.sSQL)
		F.Data.DataTable.SetValue("dtWC",-1,"UpdPri",False)
		F.Intrinsic.UI.Msgbox("Priorities Updated.","Complete")
	F.Intrinsic.Control.EndIf

	F.Intrinsic.Control.Catch
		F.Intrinsic.String.Build("Project: {0}{1}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}{1}GAB Version: {5}",V.Ambient.ScriptPath,V.Ambient.Newline,V.Ambient.CurrentSubroutine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Ambient.GABVersion,V.Local.sError)
		F.Intrinsic.UI.Msgbox(V.Local.sError)
		F.Intrinsic.Control.End 
	F.Intrinsic.Control.EndTry
Program.Sub.cmdUpdPri_Click.End

Program.Sub.ddlWG_SelectedIndexChanged.Start
F.Intrinsic.Control.Try
	V.Local.sError.Declare
	
	V.Local.sSQL.Declare
	V.Local.sRet.Declare
	V.Local.dRet.Declare
	V.Local.iCnt.Declare
	V.Local.sJob.Declare
	V.Local.sSuffix.Declare
	V.Local.sSeq.Declare
	V.Local.sSPad.Declare
	V.Local.sPrevSeq.Declare
	V.Local.fPrevQty.Declare
	V.Local.sPrevClsd.Declare
	v.Local.sCodesort.Declare

	Gui.frmResched.lblStatus.Caption("Loading Work Orders....")
	Gui.frmResched.lblStatus.Visible(True)
	Gui.frmResched.pbStatus.Visible(True)

	F.Intrinsic.Control.If(V.DataTable.dtWC.Exists)
		F.Data.DataTable.Close("dtWC")
	F.Intrinsic.Control.EndIf
			
	V.Global.bLoadData.Set(True)

	F.Intrinsic.Control.If(V.Screen.frmResched!optWC.Value)
		F.Intrinsic.String.Build("SELECT DISTINCT CAST(0 As BIT) AS Sel,IF(A.SFX = '',A.JOB,A.JOB + '-' + A.SFX) AS WO,CAST(0 As BIT) AS UpdPri,B.PRIORITY AS PRIORITY2,A.SEQ,RTRIM(C.SAR) AS WC,'' AS NewWC,ROG AS DEPT,COALESCE(B.PART,'') AS PART,'' As DISPLAY_PART,B.PART_DESCRIPTION,A.STARTDATE,A.DUEDATE,B.DATE_START,B.DATE_DUE AS DATE_DUE2,B.SCHEDULED_DUE_DATE AS SCHED_DATE,A.INITSU+A.HOURSORIG AS EST_HRS,HOURSAPPL AS ACT_HRS,IF(A.INITSU+A.HOURSORIG = 0,0,(A.INITSU+A.HOURSORIG)- HOURSAPPL) AS REM_HRS,A.PCSTOPROD AS EST_QTY,A.PCSREMAIN AS REM_QTY,CONVERT(0,SQL_FLOAT) AS PrevQty,A.JOB,A.SFX,B.PRIORITY,B.DATE_DUE,'' AS PrevSeq,'' AS PrevClsd,B.PART+B.LOCATION AS PARTLOC,B.JOB+B.SUFFIX AS JOBSUF,CAST(RTRIM(B.JOB+B.SUFFIX+REPLICATE('0',6-LENGTH(A.SEQ))+CAST(A.SEQ AS CHAR(6))) AS CHAR(15)) AS JOBSUFLBR, '' as CODE_SORT_OPER FROM APSV3_JBLINES A LEFT JOIN V_JOB_HEADER B ON A.JOB = B.JOB AND A.SFX = B.SUFFIX LEFT JOIN APSV3_UBDETAIL C ON A.JSS = C.JSS WHERE A.SEQTYPE = '1' AND C.SAR = '{0}' AND COMPLDATE IS NULL AND A.JOBTYPE <> 2 AND B.DATE_CLOSED = '1900-01-01' ORDER BY A.STARTDATE,A.JOB,A.SFX,A.SEQ;",V.Screen.frmResched!ddlWC.Text,V.Local.sSQL)
		F.Data.DataTable.CreateFromSQL("dtWC","con",V.Local.sSQL,True)
	F.Intrinsic.Control.Else
		F.Intrinsic.String.Build("SELECT CAST(0 As BIT) AS Sel,IF(A.SFX = '',A.JOB,A.JOB + '-' + A.SFX) AS WO,CAST(0 As BIT) AS UpdPri,B.PRIORITY AS PRIORITY2,A.SEQ,RTRIM((SELECT TOP 1 SAR FROM APSV3_UBDETAIL WHERE A.JSS = APSV3_UBDETAIL.JSS)) AS WC,'' AS NewWC,RTRIM((SELECT TOP 1 ROG FROM APSV3_UBDETAIL WHERE A.JSS = APSV3_UBDETAIL.JSS)) AS DEPT,COALESCE(B.PART,'') AS PART,'' As DISPLAY_PART,B.PART_DESCRIPTION,A.STARTDATE,A.DUEDATE,B.DATE_START,B.DATE_DUE AS DATE_DUE2,B.SCHEDULED_DUE_DATE AS SCHED_DATE,A.INITSU+A.HOURSORIG AS EST_HRS,HOURSAPPL AS ACT_HRS,IF(A.INITSU+A.HOURSORIG = 0,0,(A.INITSU+A.HOURSORIG)- HOURSAPPL) AS REM_HRS,A.PCSTOPROD AS EST_QTY,A.PCSREMAIN AS REM_QTY,CONVERT(0,SQL_FLOAT) AS PrevQty,A.JOB,A.SFX,B.PRIORITY,B.DATE_DUE,'' AS PrevSeq,'' AS PrevClsd,B.PART+B.LOCATION AS PARTLOC,B.JOB+B.SUFFIX AS JOBSUF,CAST(RTRIM(B.JOB+B.SUFFIX+REPLICATE('0',6-LENGTH(A.SEQ))+CAST(A.SEQ AS CHAR(6))) AS CHAR(15)) AS JOBSUFLBR,  '' as CODE_SORT_OPER FROM APSV3_JBLINES A LEFT JOIN V_JOB_HEADER B ON A.JOB = B.JOB AND A.SFX = B.SUFFIX WHERE A.SEQTYPE = '1' AND A.SEQRES = '{0}' AND A.COMPLDATE IS NULL AND A.JOBTYPE <> 2 AND WC IS NOT NULL AND B.DATE_CLOSED = '1900-01-01' ORDER BY A.STARTDATE,A.JOB,A.SFX,A.SEQ;",V.Screen.frmResched!ddlWG.Text,V.Local.sSQL)
		F.Data.DataTable.CreateFromSQL("dtWC","con",V.Local.sSQL,True)
	F.Intrinsic.Control.EndIf
	
	f.Intrinsic.Control.If(v.Global.bLongPart)
		F.Data.DataTable.AddDisplayPartColumn("dtWC",0,"PART","DISPLAY_PART")
	f.Intrinsic.Control.Else
		F.Data.DataTable.CopyColumn("dtWC","PART","DISPLAY_PART")
	f.Intrinsic.Control.EndIf	

	Gui.frmResched.pbStatus.Min(0)

	F.Intrinsic.Control.If(V.DataTable.dtWC.RowCount,<>,0)
		Gui.frmResched.pbStatus.Max(V.DataTable.dtWC.RowCount)
		Gui.frmResched.pbStatus.Value(0)
	F.Intrinsic.Control.Else
		Gui.frmResched.pbStatus.Value(0)
	F.Intrinsic.Control.EndIf

	F.Intrinsic.Control.For(V.Local.iCnt,0,V.DataTable.dtWC.RowCount--,1)
		Gui.frmResched.pbStatus.Value(V.Local.iCnt)
		V.Local.sJob.Set(V.DataTable.dtWC(V.Local.iCnt).JOB!FieldVal)
		V.Local.sSuffix.Set(V.DataTable.dtWC(V.Local.iCnt).SFX!FieldVal)
		V.Local.sSeq.Set(V.DataTable.dtWC(V.Local.iCnt).SEQ!FieldVal)
		f.Intrinsic.String.LPad(v.Local.sSeq,"0",6,v.Local.sSPad)
		F.Intrinsic.String.Build("SELECT RTRIM(CODE_SORT_OPER) FROM V_JOB_OPERATIONS WHERE JOB = '{0}' AND SUFFIX = '{1}' AND SEQ = '{2}' AND LMO = 'L';",V.Local.sJob,V.Local.sSuffix,V.Local.sSPad,V.Local.sSQL)
		F.ODBC.Connection!con.ExecuteAndReturn(V.Local.sSQL,V.Local.sCodeSort)

		
		F.Intrinsic.String.Build("SELECT TOP 1 SEQ,UNITS_COMPLETE,FLAG_CLOSED FROM V_JOB_OPERATIONS WHERE JOB = '{0}' AND SUFFIX = '{1}' AND SEQ < {2} AND LMO = 'L' ORDER BY SEQ DESC;",V.Local.sJob,V.Local.sSuffix,V.Local.sSeq,V.Local.sSQL)
		F.ODBC.Connection!con.ExecuteAndReturn(V.Local.sSQL,V.Local.sRet)
		F.Intrinsic.String.Split(V.Local.sRet,"*!*",V.Local.sRet)
		V.Local.sPrevSeq.Set(V.Local.sRet(0))
		V.Local.fPrevQty.Set(V.Local.sRet(1).Float)
		V.Local.sPrevClsd.Set(V.Local.sRet(2))
		F.Intrinsic.Control.If(V.Local.sPrevClsd,=,"OOB")
			V.Local.sPrevClsd.Set("")
		F.Intrinsic.Control.EndIf
		F.Data.DataTable.SetValue("dtWC",V.Local.iCnt,"PrevSeq",V.Local.sPrevSeq,"PrevQty",V.Local.fPrevQty,"PrevClsd",V.Local.sPrevClsd,"CODE_SORT_OPER",v.Local.sCodesort)
			
		'DATE_START
		F.Intrinsic.String.Build("SELECT TOP 1 STARTDATE FROM APSV3_JBLINES WHERE JOB = '{0}' AND SFX = '{1}' AND SEQTYPE = 1 ORDER BY STARTDATE;",V.Local.sJob,V.Local.sSuffix,V.Local.sSQL)
		f.Data.Datatable.CreateFromSQL("dtStartDate","con",v.Local.sSQL)
		f.Intrinsic.Control.If(v.DataTable.dtStartDate.RowCount,>,0)
			F.Data.DataTable.SetValue("dtWC",V.Local.iCnt,"DATE_START",Variable.DataTable.dtStartDate(0).StartDate!FieldVal)		
		f.Intrinsic.Control.EndIf
		f.Data.Datatable.Close("dtStartDate")
	F.Intrinsic.Control.Next(V.Local.iCnt)
	
	'Add Columns
		'Sort Code
		F.Data.DataTable.AddColumn("dtWC","SortCode",String,"")
		F.Data.Dictionary.CreateFromSQL("dSortCode","con","Select PART+LOCATION AS PARTLOC, RTRIM(CODE_SORT) AS CODE_SORT From V_INVENTORY_MSTR Where CODE_SORT <> '' Order By PART, LOCATION;")
		F.Data.Dictionary.SetDefaultReturn("dSortCode","")
		F.Data.DataTable.FillFromDictionary("dtWC","dSortCode","PARTLOC","SortCode")
		F.Data.Dictionary.Close("dSortCode")
		'Flex Schedule
		F.Data.DataTable.AddColumn("dtWC","FlexSched",String,"")
		F.Data.Dictionary.CreateFromSQL("dFlexSched","con","Select RTRIM(JOB+SUFFIX+LABOR_SEQ) AS JOBSUFLBR, SCHEDULE+'-'+SCHEDULE_SUFFIX AS FLEX_SCHED From JOB_CT_FLX_SCHD Where SCHEDULE <> '' Order By SCHEDULE, SCHEDULE_SUFFIX;")
		F.Data.Dictionary.SetDefaultReturn("dFlexSched","")
		F.Data.DataTable.FillFromDictionary("dtWC","dFlexSched","JOBSUFLBR","FlexSched")
		F.Data.Dictionary.Close("dFlexSched")

	F.Intrinsic.Control.BlockEvents
	F.Intrinsic.Control.CallSub(LoadGVWC)
	F.Intrinsic.Control.CallSub(Deserialize)
	Gui.frmResched.chkUpdPri.Enabled(True)
	F.Intrinsic.Control.UnBlockEvents

	Gui.frmResched.lblStatus.Visible(False)
	Gui.frmResched.pbStatus.Visible(False)
	
	F.Intrinsic.Control.Catch
		F.Intrinsic.String.Build("Project: {0}{1}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}{1}GAB Version: {5}",V.Ambient.ScriptPath,V.Ambient.Newline,V.Ambient.CurrentSubroutine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Ambient.GABVersion,V.Local.sError)
		F.Intrinsic.UI.Msgbox(V.Local.sError)
		F.Intrinsic.Control.End 
	F.Intrinsic.Control.EndTry
Program.Sub.ddlWG_SelectedIndexChanged.End

